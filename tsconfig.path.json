    "baseUrl": "./src",
    "paths": {
      "@pages": [
        "pages"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@entities": [
